/*4_1会员中心-会员权益*/
#vip-ban{
    width: 100%;
    padding-top:21%;
    background-position: center center;
    -webkit-background-size: cover;
    -moz-background-size: cover;
    -o-background-size: cover;
    background-size: cover;
    background-repeat: no-repeat;
}
.vip-bg{
    background: #f2efe5;
}
.vip-tit{
    float: left;
    width: 25%;
    margin-top: -27px;
}
.vip-txt{
    float:right;
    width: 75%;
}
.vip-txt .vip-pad{
    padding: 27px 30px;
}
.vip-txt h5{
    font-size: 18px;
    color:#a96b3a;
    font-weight: normal;
    margin-bottom: 30px;
} 
.vip-txt  p{
    font-size: 14px;
    color:#000;
    line-height: 30px;
    margin-bottom: 30px;
}
.vip-txt span{
    float: right;
    display: block;
    margin: 28px 0;
}

/*4度假酒店*/
.jd-tit-bg{
    background: #f5f2e9;
    padding-top: 25px;
}
.jd-txt{
    padding: 30px 0;
    line-height: 35px;
    text-align: center;
    font-size: 16px;
}
.ul-jd li{
    width: 100%;
    height: 300px;
    background-repeat: no-repeat;
    background-position: center center;
    -webkit-background-size: cover;
    background-size: cover;
    position: relative;
}
.ul-jd .on-bg{
    position: absolute;
    top:50%;
    left:50%;
    margin-top: -103px;
    margin-left: -103px;
    width: 206px;
    height: 206px;
    background: #fff;
    border-radius: 120px;
    text-align: center;
    background: rgba(255,255,255,.7);
    padding-top: 20px;
    -o-transition:.3s;
    -ms-transition:.3s;
    -moz-transition:.3s;
    -webkit-transition:.3s;
    transition:.3s;
}
.ul-jd li .on-bg:hover{
    background: rgba(255,255,255,1);
    transform: translateX(-20px);
    -o-transform:translateX(-20px);
    -ms-transform:translateX(-20px);
    -moz-transform:translateX(-20px);
    -webkit-transform:translateX(-20px);
}
.ul-jd .jd-online{
    margin-top: 10px;
}
.ul-jd .jd-online a{
    display: inline-block;
    width: 120px;
    height: 35px;
    line-height: 35px;
    background: url(../images/on-li-ico.png) no-repeat 17px center #8e6141;
    font-size: 16px;
    color:#fff;
    padding-left: 20px;
}

/*6活动掠影_详情*/
.new-bg{
    background: #eeeeee;
    padding-top: 38px;
}
.new-data{
    text-align: center;
    font-size: 16px;
    color:#555555;
    padding:20px 0 40px 0;
}
.new-data span{
    border-right:1px solid #d7d7d7;
    padding-right: 20px;
    margin-right: 20px;
}
.new-data em{
    display: inline-block;
    background: url(../images/new-data.png) no-repeat left;
    padding-left: 30px;
}
.new-pad{
    padding:58px 93px;
    border-bottom: 1px solid #dcdcdc;
    overflow: hidden;
}
.new-txt{
    font-size: 14px;
    color:#555555;
    line-height: 25px;
}
.new-txt p{
    margin-bottom: 28px;
}

.new-pic .new-img1{
    width: 64%;
}
.new-pic .new-img2{
    width: 35%;
}
.new-pic .new-img1 img.
.new-pic .new-img2 img{
    width: 100%;
}
.new-pad-ll{
    padding: 10px 38px 63px 38px;
    overflow: hidden;
    
}
.new-pad-ll .page{
    float:left;
    font-size: 16px;
}
.new-pad-ll .page p{
    line-height: 30px;
}
.new-pad-ll .page a:hover{
    color:#ad7344;
}
.new-pad-ll .return{
    float:right;
    width: 135px;
    height: 63px;
    line-height: 63px;
    background: url(../images/return-bg.png) no-repeat;
    font-size: 14px;
    padding-left: 15px;
}
.new-pad-ll .return a{
    display: inline-block;
    background: url(../images/return-ico.png) no-repeat left center;
    padding-left: 15px;
    color:#fff;
}

/*5_2会员中心-1会员登录*/
.vip-login-bg{
    width: 100%;
    height: 100%;
    padding-top:43%;
    background-repeat: no-repeat;
    -webkit-background-size: cover;
    background-size: cover;
    background-position: center center;
    position: relative;

}
.vip-login{
    width: 486px;
    height: 438px;
    background: #fff;
    position: absolute;
    top:50%;
    left:50%;
    margin-top: -243px;
    margin-left: -219px;
    background: rgba(255,255,255,.9);
}






