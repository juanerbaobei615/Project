.mht-succ .txt h4{font-size: 24px;color: #b9875f;border-bottom: 1px solid #dcdcdc;padding-left:5px;padding-bottom: 8px;
    margin-bottom:13px; margin-top:22px;
}
.mht-succ .txt p{font-size: 18px;color: #555;}
.mht-succ .txt p span{color: #9a9a9a;line-height:40px;}
