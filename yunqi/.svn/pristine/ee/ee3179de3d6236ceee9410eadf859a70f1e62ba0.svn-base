/*3民宿预订-2支付*/
.mht-pay{margin-top: 60px;padding-bottom: 170px;}
.mht-pay .pay-rem{overflow: hidden;background-color: #f5f5f5;text-align: center;margin-bottom: 45px;}
.mht-pay .pay-rem p{font-size: 18px;color: #b9875f;line-height: 60px;}
.mht-pay .pay-rem p img{vertical-align: middle;}
.mht-pay .pay-cen{max-width: 480px;margin: 0 auto;}
.mht-pay .pay-cen p{font-size: 18px;color: #555;line-height:60px;}
.mht-pay .pay-cen p span{float: left;}
.mht-pay .pay-cen p em{display: inline-block;font-size: 30px;color: #b9875f;float: left;margin-left:8px;}
.mht-pay .pay-cen p .cz{display: inline-block;float: right;background-color: #b9875f;font-size: 16px;color: #fff;
    margin-top: 13px;width: 105px;text-align: center;height:40px;line-height:40px;
}
.mht-pay .pay-cen p input{padding: 0 10px;margin: 0 5px;border: 1px solid #dcdcdc;}
.mht-pay .pay-cen p .inp1{width: 300px;height: 40px;font-size: 18px;color: #555;}
.mht-pay .pay-cen p .inp2{width: 195px;height: 50px;font-size: 14px;color: #a9a9a9;}
.mht-pay .pay-cen p .yzm{background-color: #c2b0a3;color: #fff;font-size: 12px;padding:17px 25px;}
.mht-pay .pay-cen p .yzm:hover{background-color: #b9875f;}
.mht-pay .pay-cen .zf{text-align: center;margin-top: 45px;}
.mht-pay .pay-cen .zf a{background-color: #b9875f;font-size: 24px;color: #fff;padding: 10px 155px;}


/*3民宿预订-1预定信息*/
