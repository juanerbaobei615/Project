.mht-succ{margin-bottom: 45px;}
.mht-succ .tit{text-align:right;max-width:450px;margin: 0 auto;padding-top: 36px;}

.mht-succ .tit h3{font-size: 30px;color: #b9875f;background: url(../images/succ-i1.png) no-repeat left center;}
.mht-succ .txt h4{font-size: 24px;color: #b9875f;border-bottom: 1px solid #dcdcdc;padding-left:5px;padding-bottom: 8px;
