<link rel="stylesheet" href="css/jquery.fullPage.css">
<script src="js/jquery.fullPage.min.js"></script>

<script>
function opa(){
    var winW = $(window).width();
    if(winW > 960){
        $('[ani*="animated"]').css('opacity',0);
    }else{
        $('[ani*="animated"]').css('opacity',1);
    }
}
opa();

$(document).ready(function($) {
    $('#fullpage').fullpage({
        'navigation': true,
        // 'anchors': ['page1', 'page2', 'page3'],
        // 'fixedElements': '#mobile-menu',
        'paddingTop':'122px',
        // verticalCentered: !1,
        // navigation: !0,
        'anchors': ['page1', 'page2', 'page3', 'page4'],
        slidesNavigation: true,
        slidesNavPosition: 'bottom',
        easingcss3: 'ease',
        afterLoad: function(anchorLink, index){
            var winW = $(window).width();
            if(winW > 960){
                if(index == 4){
                    $('[ani*="animated"]').css('opacity',0);
                }else{
                    $(".section").eq(index-1).find('[ani*="animated"]').each(function(){
                        var _this = $(this);
                        var _class = $(this).attr('ani');
                        var _time = $(this).attr('ani-time');
                        setTimeout(function(){
                           _this.stop().addClass(_class);
                        },_time);
                    });
                    $(".section").not(".active").find('[ani*="animated"]').each(function(){
                        var _this = $(this);
                        var _class = $(this).attr('ani');
                        _this.removeClass(_class);
                        _this.css('opacity',0);
                    });
                }
            }
        }
    });

    setInterval(function(){
        $.fn.fullpage.moveSlideRight();
    }, 5000);

    $(window).resize(function(){
        autoScrolling();
    });

    function autoScrolling(){
        var $ww = $(window).width();
        if($ww > 767){
            $.fn.fullpage.setAutoScrolling(true);
            $.fn.fullpage.setAllowScrolling(true);
        } else {
            $.fn.fullpage.setAutoScrolling(false);
            $.fn.fullpage.setAllowScrolling(false);
            $.fn.fullpage.destroy();
        }
    }
    autoScrolling();

});

</script>

