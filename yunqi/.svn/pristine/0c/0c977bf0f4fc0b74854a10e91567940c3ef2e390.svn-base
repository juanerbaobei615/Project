/*4度假酒店*/
.jd-tit-bg{
    background: #f5f2e9;
    padding-top: 25px;
}
.jd-txt{
    padding: 30px 0;
    line-height: 35px;
    text-align: center;
    font-size: 16px;
}
.ul-jd li{
    width: 100%;
  /*  height: 300px;*/
    overflow: hidden;    
    position: relative;    
}
.ul-jd li .bg{
    background-repeat: no-repeat;
    background-position: center center;
    -webkit-background-size: cover;
    background-size: cover;
}
.ul-jd li .bg div{height: 300px;}
.ul-jd li:hover .bg{
    -o-transform:scale(1.15);
    -ms-transform:scale(1.15);
    -moz-transform:scale(1.15);
    -webkit-transform:scale(1.15);
    transform:scale(1.15);
}
.ul-jd li .bg{
    -o-transition:3s;
    -ms-transition:3s;
    -moz-transition:3s;
    -webkit-transition:3s;
    transition:3s;
}
.ul-jd .on-bg{
    position: absolute;
    top:50%;
    left:50%;
    margin-top: -103px;
    margin-left: -103px;
    width: 206px;
    height: 206px;
    background: #fff;
    border-radius: 120px;
    text-align: center;
    background: rgba(255,255,255,.9);
    padding-top: 20px;
    -o-transition:.3s;
    -ms-transition:.3s;
    -moz-transition:.3s;
    -webkit-transition:.3s;
    transition:.3s;
}
.ul-jd li:hover .on-bg{
    background: rgba(255,255,255,.7);
    transition: all 1s ease 0.3s;
}
.ul-jd .jd-online{
    margin-top: 10px;
}
.ul-jd .jd-online a{
    display: inline-block;
    width: 120px;
    height: 35px;
    line-height: 35px;
   background:url(../images/on-li-ico2.png) no-repeat 17px center ;
    font-size: 16px;
    border: 1px solid #8e6141;
    padding-left: 20px;
    color: #8e6141;
}
.ul-jd li:hover .jd-online a{    
    background: url(../images/on-li-ico.png) no-repeat 17px center #8e6141;
    color:#fff;   
    -o-transition:1s;
    -ms-transition:1s;
    -moz-transition:1s;
    -webkit-transition:1s;
    transition:1s;
}
