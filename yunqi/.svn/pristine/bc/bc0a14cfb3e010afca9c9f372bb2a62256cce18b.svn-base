/*3民宿预订-2预订成功*/
.mht-succ{margin-bottom: 45px;}
.mht-succ .tit{text-align:right;max-width:450px;margin: 0 auto;padding-top: 36px;}

.mht-succ .tit h3{font-size: 30px;color: #b9875f;background: url(../images/succ-i1.png) no-repeat left center;}
.mht-succ .txt h4{font-size: 24px;color: #b9875f;border-bottom: 1px solid #dcdcdc;padding-left:5px;padding-bottom: 8px;
    margin-bottom:13px; margin-top:22px;
}
.mht-succ .txt p{font-size: 18px;color: #555;}
.mht-succ .txt p span{color: #9a9a9a;line-height:40px;}
.mht-succ .txt p a{display: inline-block;font-size: 24px;color: #fff;background-color: #b9875f;padding:7px 135px;
    margin-top: 3px;margin-bottom: 7px;
}
.mht-succ .txt p span b{font-weight: normal;color: #b9875f;}
/*3民宿预订-2支付*/
.mht-pay{margin-top: 60px;padding-bottom: 170px;}
.mht-pay .pay-rem{overflow: hidden;background-color: #f5f5f5;text-align: center;margin-bottom: 45px;}
.mht-pay .pay-rem p{font-size: 18px;color: #b9875f;line-height: 60px;}
.mht-pay .pay-rem p img{vertical-align: middle;}
.mht-pay .pay-cen{width: 480px;margin: 0 auto;}
.mht-pay .pay-cen p{font-size: 18px;color: #555;line-height:60px;}
.mht-pay .pay-cen p span{float: left;}
.mht-pay .pay-cen p em{display: inline-block;font-size: 30px;color: #b9875f;float: left;margin-left:8px;}
.mht-pay .pay-cen p .cz{float: right;background-color: #b9875f;font-size: 16px;color: #fff;padding: 0 20px;line-height:34px;
    margin-top: 13px;
}
.mht-pay .pay-cen p input{padding: 0 10px;margin: 0 5px;border: 1px solid #dcdcdc;}
.mht-pay .pay-cen p .inp1{width: 300px;height: 40px;font-size: 18px;color: #555;}
.mht-pay .pay-cen p .inp2{width: 195px;height: 50px;font-size: 14px;color: #a9a9a9;}
.mht-pay .pay-cen p .yzm{background-color: #c2b0a3;color: #fff;font-size: 12px;padding:17px 25px;}
.mht-pay .pay-cen p .yzm:hover{background-color: #b9875f;}
.mht-pay .pay-cen .zf{text-align: center;margin-top: 45px;}
.mht-pay .pay-cen .zf a{background-color: #b9875f;font-size: 24px;color: #fff;padding: 10px 155px;}


/*3民宿预订-1预定信息*/
.mht-infor .tit{border-bottom: 1px solid #dcdcdc;margin-top:35px;margin-bottom: 30px;}
.mht-infor .tit h3{color: #b9875f;font-size: 24px;padding-left: 5px;padding-bottom:6px;}
.mht-infor .txt p{font-size: 18px;color: #555; padding-left: 105px;line-height:65px;}
.mht-infor .txt p em{display: inline-block;width: 75px;text-align: center;}
.mht-infor .txt p input{margin: 0 5px;padding: 0 10px;}
.mht-infor .txt p .t-inp{width: 280px;height: 40px;font-size: 18px;color: #555;border: 1px solid #dcdcdc;
    background: url(../images/date-i1.png) no-repeat right 5px center; 
}
.mht-infor .txt p select{width: 280px;height: 40px;border: 1px solid #dcdcdc;font-size: 18px;color: #555;margin-left: 5px;
    background: url(../images/xxll.png) no-repeat right 13px center;
}
.mht-infor .txt p b{font-size: 40px;color: #b9875f;font-weight: normal;}
.mht-infor .txt p span{color: #9a9a9a;}
.mht-infor .txt p .cz{background-color: #b9875f;font-size: 16px;color: #fff;padding: 5px 33px;}
.mht-infor .txt p .inp2{width: 280px;height: 40px;font-size: 18px;color: #555;border: 1px solid #dcdcdc;}
.mht-infor .tab1{border: 1px solid #eee;width: 100%;}
.mht-infor .tab1 tr{height: 43px;}
.mht-infor .tab1 .tr1 td{background-color: #f5f5f5;font-size: 14px;color: #000;}
.mht-infor .tab1 .tr1 .td0{width: 5%;}
.mht-infor .tab1 .tr1 .td1{width: 14.75%;}
.mht-infor .tab1 .tr1 .td2{width: 22%;}
.mht-infor .tab1 .tr1 .td4{width: 16%;}
.mht-infor .tab1 .tr1 .td5{width: 11%;  }
.mht-infor .tab1 .tr2{font-size: 14px;color: #555;}
.mht-infor .txt2{margin-bottom:35px;}
.mht-infor .txt2 .p1{border-bottom: 1px dashed #eee;border-top:  1px dashed #eee;margin-top: 30px;}
.mht-infor .txt2 p{font-size: 14px;color: #555;text-align: right;height: 60px;line-height:70px;}
.mht-infor .txt2 p em{display: inline-block;width: 60px;text-align: left;}
.mht-infor .txt2 p span{display: inline-block;width: 100px;text-align: right;}
.mht-infor .txt2 .p1 span b{font-size:18px;}
.mht-infor .txt2 .p2{margin-bottom: 15px;}
.mht-infor .txt2 .p2 span b{font-size: 40px;font-weight: normal;color: #b9875f;}
.mht-infor .txt2 p a{background-color: #b9875f;font-size: 22px;color: #fff;
    padding:10px 135px;
}




